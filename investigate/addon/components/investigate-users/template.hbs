{{#investigate-header activeTab='investigate.investigate-users' as |section|}}
  {{#if section.isMain}}
    {{#if hasUEBAError}}
      <a href="https://www.rsa.com/en-us/products/threat-detection-response/ueba" target="_blank" >
        <img test-id="uebaUnavailable" class="ueba-unavailable" src="/assets/images/ueba-unavailable.png">
      </a>
    {{else}}
      {{#unless iframeVisible}}
        {{ueba-loader}}
      {{/unless}}
      {{ueba-iframe iframeVisible=iframeVisible testId="uebaIframe" handleOnLoad=(action "iframeLoad")}}
    {{/if}}
    {{outlet}}
  {{/if}}
{{/investigate-header}}
