<article class="rsa-investigate js-test-investigate-root">
  <nav class="rsa-investigate__nav">
    <ul>
      <li>
        <a href="/investigation" class="js-test-investigate-navigate-classic">Navigate</a>
      </li>
      <li>
        <a href="/investigation/events" class="js-test-investigate-events-classic">Events</a>
      </li>
      <li>
        <a href="/investigation/malware" class="js-test-investigate-malware-classic">Malware Analysis</a>
      </li>
    </ul>
    <div class="investigate-controls">
      {{rsa-icon
        active=(not-eq model.recon.size 'full')
        disabled=(not model.recon.isOpen)
        click=(route-action 'toggleSlaveFullScreen')
        name='layout-1'
        title=(t 'investigate.controls.toggle')}}
      <div class="divider"></div>
      {{rsa-icon
        disabled=true
        name='cog'
        title=(t 'investigate.controls.settings')}}
    </div>
  </nav>
  {{outlet}}
</article>
