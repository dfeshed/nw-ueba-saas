{{#if session.isAuthenticated}}
  <page class="rsa-protected {{if (and entityId entityType) 'has-entity-info'}}">
    <page class="rsa-protected__outlet">
      {{outlet}}
    </page>
    <aside class="rsa-protected__aside">
      {{rsa-context-panel entityId=entityId entityType=entityType closePanel=(route-action 'closeContextPanel')}}
    </aside>
  </page>
{{/if}}
