<div id="modalDestination"></div>
{{#rsa-application-modal
  label="Critical Error"
  style='error'
  eventId='fatalError'
  modalDidClose='clearFatalErrorQueue'}}
  <div class='modal-content'>
    {{#each fatalErrors.queue as |error|}}
      <p>{{error}}</p>
    {{/each}}
  </div>
{{/rsa-application-modal}}

{{#if session.isAuthenticated}}
  {{#rsa-application-header version=version title=title}}
    {{#if features.showRespondRoute}}
      {{#link-to "protected.respond" class="rsa-header-nav-respond js-test-nav-respond-link"}}{{t "respond.title"}}{{/link-to}}
    {{/if}}
    {{#if features.showInvestigateRoute}}
      {{#link-to "protected.investigate" class="rsa-header-nav-investigate js-test-nav-investigate-link"}}{{t "investigate.title"}}{{/link-to}}
    {{/if}}
    <a href="/unified/dashboard" class="rsa-header-nav-monitor js-test-nav-monitor-link">{{t "monitor.title"}}</a>
    <a href="/live/search" class="rsa-header-nav-configure js-test-nav-configure-link">{{t "config.title"}}</a>
    <a href="/admin/appliances" class="rsa-header-nav-admin js-test-nav-admin-link">{{t "admin.title"}}</a>
  {{/rsa-application-header}}

  {{rsa-application-action-bar}}

  {{#rsa-application-content}}
    {{outlet}}
  {{/rsa-application-content}}
{{else}}
  {{outlet}}
{{/if}}


{{rsa-application-notifications-panel}}
{{rsa-application-incident-queue-panel}}
