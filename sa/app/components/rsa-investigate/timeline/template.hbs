<header class="rsa-investigate-timeline__header">
  <h2>{{t "investigate.events.title"}}</h2>
</header>
<section class="rsa-investigate-timeline__body">
  {{#rsa-chart
    data=chartData
    margin=chartMargin
    xScaleFn=xScaleFn
    xDomain=xDomain
    xProp=xProp
    yProp=yProp
    as |data xScale yScale graphWidth graphHeight hoverData|
  }}
    {{rsa-chart-grids
      xScale=xScale
      yScale=yScale
      yTickCount=6
      width=graphWidth
      height=graphHeight
    }}
    {{rsa-line-series
      data=data
      dataIndex=1
      xScale=xScale
      yScale=yScale
      xProp=xProp
      yProp=yProp
      hoverData=hoverData}}
    {{rsa-line-series
      data=data
      dataIndex=0
      xScale=xScale
      yScale=yScale
      xProp=xProp
      yProp=yProp
      hoverData=hoverData
    }}
    {{rsa-x-axis height=graphHeight scale=xScale}}
    {{rsa-y-axis scale=yScale tickCount=6}}
  {{/rsa-chart}}
</section>
