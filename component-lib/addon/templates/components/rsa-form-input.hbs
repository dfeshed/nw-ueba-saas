<span class="rsa-form-label">
  {{label}}
  
  {{#if isError}}
    {{#if errorMessage}}
      {{rsa-icon name="report-problem-triangle" title=errorMessage class="error-icon" size='large'}}
    {{/if}}
  {{/if}}
</span>

{{input
  type=type
  value=value
  placeholder=placeholder
  disabled=resolvedDisabled}}
