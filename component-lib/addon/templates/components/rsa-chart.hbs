<svg width="100%" height="100%">
  <g transform="translate({{margin.left}},{{margin.top}})">
    <rect class="rsa-chart-background" width="{{graphWidth}}" height="{{graphHeight}}"></rect>
    {{yield (hash
      data=data
      xScale=xScale
      yScale=yScale
      graphWidth=graphWidth
      graphHeight=graphHeight
      hoverData=hoverData
      duration=duration)
    }}
  </g>
</svg>
