{{#if isEditing}}
  {{query-container/profile-selector/profile-details/profile-form
    columnGroups=columnGroups
    metaGroups=metaGroups
    profile=profile
    profiles=profiles
    selectedColumnGroupId=selectedColumnGroupId
    sendToBroadcast=(action 'broadcastChangedProfile')
  }}
