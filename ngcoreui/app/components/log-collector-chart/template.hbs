<h1 class="large centered">Log Collector Overall Stats</h1>
{{#rsa-chart domainExtents=domainExtents data=getLogCollectorEPS as |params|}}
  {{rsa-line-series
    data=params.data
    dataIndex=0
    xScale=params.xScale
    yScale=params.yScale
    height=params.graphHeight}}
  {{rsa-line-series
    data=params.data
    dataIndex=1
    xScale=params.xScale
    yScale=params.yScale
    height=params.graphHeight}}
  {{rsa-line-series
    data=params.data
    dataIndex=2
    xScale=params.xScale
    yScale=params.yScale
    height=params.graphHeight}}
  {{rsa-y-axis scale=params.yScale tickCount=3}}
{{/rsa-chart}}
<div>
  <h1 class="color-box-0"> LC Incoming EPS </h1>
  <h1 class="color-box-1"> LC Outgoing EPS </h1>
  <h1 class="color-box-2"> TCP Connector Rate </h1>
</div>