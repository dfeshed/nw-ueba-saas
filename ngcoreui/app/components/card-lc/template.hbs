<h1 class="large centered bottom-padding">Log Collector Statistics</h1>
{{#rsa-data-table items=itemsList columnsConfig=(readonly columns)}}
  {{#rsa-data-table/header as |column|}}
    {{column.title}}
  {{/rsa-data-table/header}}
  {{#rsa-data-table/body noResultsMessage='No EventSources Found' as |item index column|}}
    {{#rsa-data-table/body-cell item=item column=column}}
      {{get item column.field}}
    {{/rsa-data-table/body-cell}}
  {{/rsa-data-table/body}}
{{/rsa-data-table}}