{
  "BucketConfs": [
    {
      "name": "normalized_username_kerberos_logins_daily",
      "contextFieldNames": [
        "normalized_username"
      ],
      "dataSources": [
        "kerberos_logins"
      ],
      "strategyName": "fixed_duration_daily",
      "aggrFeatureConfs": [
        {
          "name": "src_machine_histogram",
          "featureNamesMap": {
            "groupBy": ["source_machine"]
          },
          "aggrFeatureFuncJson": {"type": "aggr_feature_histogram_func"}
        },
        {
          "name": "dst_machine_histogram",
          "featureNamesMap": {
            "groupBy": ["destination_machine"]
          },
          "aggrFeatureFuncJson": {"type": "aggr_feature_histogram_func"}
        },
        {
          "name": "dst_machine_with_high_score_histogram",
          "featureNamesMap": {
            "groupBy": ["destination_machine"]
          },
          "aggrFeatureFuncJson": {"type": "aggr_feature_histogram_func"},
          "filter": {"jsonPath": "[?(@.eventscore > 50)]"}
        },
        {
          "name": "number_of_events",
          "featureNamesMap": {},
          "aggrFeatureFuncJson": {"type": "aggr_feature_number_of_events_func"}
        },
        {
          "name": "number_of_failed_events",
          "featureNamesMap": {},
          "aggrFeatureFuncJson": {"type": "aggr_feature_number_of_events_func"},
          "filter": {"jsonPath": "[?(@.failure_code != '0x0')]"}
        }
      ]
    },

    {
      "name": "normalized_username_ssh_daily",
      "contextFieldNames": [
        "normalized_username"
      ],
      "dataSources": [
        "ssh"
      ],
      "strategyName": "fixed_duration_daily",
      "aggrFeatureConfs": [
        {
          "name": "src_machine_histogram",
          "featureNamesMap": {
            "groupBy": ["source_machine"]
          },
          "aggrFeatureFuncJson": {"type": "aggr_feature_histogram_func"}
        },
        {
          "name": "dst_machine_histogram",
          "featureNamesMap": {
            "groupBy": ["destination_machine"]
          },
          "aggrFeatureFuncJson": {"type": "aggr_feature_histogram_func"}
        },
        {
          "name": "number_of_events",
          "featureNamesMap": {},
          "aggrFeatureFuncJson": {"type": "aggr_feature_number_of_events_func"}
        },
        {
          "name": "number_of_failed_events",
          "featureNamesMap": {},
          "aggrFeatureFuncJson": {"type": "aggr_feature_number_of_events_func"},
          "filter": {"jsonPath": "[?(@.status == 'Failed')]"}
        }
      ]
    },

    {
      "name": "normalized_username_vpn_events_daily",
      "contextFieldNames": [
        "normalized_username"
      ],
      "dataSources": [
        "vpn"
      ],
      "strategyName": "fixed_duration_daily",
      "aggrFeatureConfs": [
        {
          "name": "number_of_failed_events",
          "featureNamesMap": {},
          "aggrFeatureFuncJson": {"type": "aggr_feature_number_of_events_func"},
          "filter": {"jsonPath": "[?(@.status == 'FAIL')]"}
        },
        {
          "name": "number_of_events",
          "featureNamesMap": {},
          "aggrFeatureFuncJson": {"type": "aggr_feature_number_of_events_func"}
        }
      ]
    }
      ]
    }
  ]
}