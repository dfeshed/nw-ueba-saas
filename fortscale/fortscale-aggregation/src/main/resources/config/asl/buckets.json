  "BucketConfs": [
    {
      "name": "normalized_username_kerberos_logins_daily",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "normalized_username"
      ],
      "dataSources": [
        "kerberos_logins"
      ],
      "strategyName": "fixed_duration_daily",
      "aggrFeatureConfs": [
        {
          "name": "source_machine_histogram",
          "featureNamesMap": {
            "groupBy": ["source_machine"]
          },
          "aggrFeatureFuncJson": {"type": "aggr_feature_histogram_func"}
        },
        {
          "name": "destination_machine_histogram",
          "featureNamesMap": {
            "groupBy": ["destination_machine"]
          },
          "aggrFeatureFuncJson": {"type": "aggr_feature_histogram_func"}
        },
        {
          "name": "source_machine_to_highest_score_map",
          "featureNamesMap": {
            "groupBy": ["source_machine"],
            "maximize": ["normalized_src_machine_score"]
          },
          "aggrFeatureFuncJson": {"type": "aggr_feature_feature_to_max_map_func"}
        },
        {
          "name": "destination_machine_to_highest_score_map",
          "featureNamesMap": {
            "groupBy": ["destination_machine"],
            "maximize": ["normalized_dst_machine_score"]
          },
          "aggrFeatureFuncJson": {"type": "aggr_feature_feature_to_max_map_func"}
        },
        {
          "name": "failure_code_histogram",
          "featureNamesMap": {
            "groupBy": ["failure_code"]
          },
          "aggrFeatureFuncJson": {"type": "aggr_feature_histogram_func"}
        },
        {
          "name": "failure_code_to_highest_score_map",
          "featureNamesMap": {
            "groupBy": ["failure_code"],
            "maximize": ["failure_codescore"]
          },
          "aggrFeatureFuncJson": {"type": "aggr_feature_feature_to_max_map_func"}
        },
        {
          "name": "events_counter",
          "featureNamesMap": {},
          "aggrFeatureFuncJson": {"type": "aggr_feature_events_counter_func"}
        },
        {
          "name": "failed_events_counter",
          "featureNamesMap": {},
          "aggrFeatureFuncJson": {"type": "aggr_feature_events_counter_func"},
          "filter": {"jsonPath": "[?(@.failure_code != '0x0')]"}
        },
        {
          "name": "high_score_date_time_events_counter",
          "featureNamesMap": {},
          "aggrFeatureFuncJson": {"type": "aggr_feature_events_counter_func"},
          "filter": {"jsonPath": "[?(@.date_time_score >= 50)]"}
        },
        {
          "name": "number_of_events_per_hour_histogram",
          "featureNamesMap": {
            "groupBy": ["hour"]
          },
          "aggrFeatureFuncJson": {"type": "aggr_feature_histogram_func"}
