<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans
		xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
						
	<bean id="featureBucketStrategyStore" class="fortscale.aggregation.feature.bucket.strategy.FeatureBucketStrategyMongoStore"/>
	<bean id="userInactivityFeatureBucketStrategyFactory" class="fortscale.aggregation.feature.bucket.strategy.UserInactivityFeatureBucketStrategyFactory"/>
	<bean id="vpnSessionFeatureBucketStrategyFactory" class="fortscale.aggregation.feature.bucket.strategy.VpnSessionFeatureBucketStrategyFactory"/>
	<bean id="bucketConfigurationService" class="fortscale.aggregation.feature.bucket.BucketConfigurationService"/>
	<bean id="dataSourcesSyncTimer" class="fortscale.aggregation.DataSourcesSyncTimer"/>
	<bean id="aggregatedFeatureEventsConfService" class="fortscale.aggregation.feature.event.AggregatedFeatureEventsConfService"/>
	<bean id="aggrFeatureFuncService" class="fortscale.aggregation.feature.functions.AggrFeatureFuncService"/>
	<bean id="aggrFeatureEventBuilderService" class="fortscale.aggregation.feature.event.AggrFeatureEventBuilderService"/>
	<bean id="aggregatedEventQueryService" class="fortscale.aggregation.feature.event.AggregatedEventQueryMongoService"/>
	<bean id="featureBucketQueryService" class="fortscale.aggregation.feature.event.FeatureBucketQueryMongoService"/>
	<bean id="mongoDbUtilService" class="fortscale.aggregation.util.MongoDbUtilService" />
	<bean id="supportingInformationDistinctEventsByTimePopulator" class="fortscale.aggregation.feature.services.historicaldata.populators.SupportingInformationDistinctEventsByTimePopulator" scope="prototype">
		<property name="aggregatedEventQueryService" ref="aggregatedEventQueryService"/>
	</bean>


    <bean id="dataQueryHelper" class="fortscale.services.dataqueries.querydto.DataQueryHelper"/>

	<bean id="aggrFeatureEventFunctionsService" class="fortscale.aggregation.feature.functions.IAggrFeatureEventFunctionsService" />
	<bean id="featureBucketsReaderService" class="fortscale.aggregation.feature.bucket.FeatureBucketsReaderService" />
	<bean id="aggrFeatureEventToSendRepository" class="fortscale.aggregation.feature.event.batch.AggrFeatureEventToSendRepository" />
</beans>
