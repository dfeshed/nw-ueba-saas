{
  "BucketConfs": [
    {
      "name": "successfulAuthenticationHistogramUserIdAuthenticationHourly",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "authentication"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "successfulAuthenticationHistogram",
          "featureNamesMap": {
            "groupBy": [
              "result"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_histogram_func"
          },
          "filter": {
            "jsonPath": "[?(@.result == 'SUCCESS')]"
          }
        }
      ]
    },
