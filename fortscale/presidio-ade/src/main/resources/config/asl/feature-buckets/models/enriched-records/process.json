{
  "BucketConfs": [
    {
      "name": "startInstantHistogramUserIdProcessDaily",
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "process"
      ],
      "strategyName": "fixed_duration_daily",
      "aggrFeatureConfs": [
        {
          "name": "startInstantHistogram",
          "featureNamesMap": {
            "groupBy": [
              "two_minute_resolution_epochtime"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_histogram_func"
          }
        }
      ]
    },
    {
      "name": "dstProcessFileNameHistogramUserIdProcessDaily",
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "process"
      ],
      "strategyName": "fixed_duration_daily",
      "aggrFeatureConfs": [
        {
          "name": "dstProcessFileNameHistogram",
          "featureNamesMap": {
            "groupBy": [
              "dstProcessFileName"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_histogram_func"
          },
          "filter": {
            "jsonPath": "[?('RECONNAISSANCE_TOOL' in @['dstProcessCategories'] && @.operationType == 'CREATE_PROCESS')]"
          }
        }
      ]
    },
    {
      "name": "userIdHistogramDstProcessFileNameProcessDaily",
      "contextFieldNames": [
        "dstProcessFileName"
      ],
      "adeEventTypes": [
        "process"
      ],
      "strategyName": "fixed_duration_daily",
      "aggrFeatureConfs": [
        {
          "name": "userIdHistogram",
          "featureNamesMap": {
            "groupBy": [
              "userId"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_histogram_func"
          },
          "filter": {
            "jsonPath": "[?('RECONNAISSANCE_TOOL' in @['dstProcessCategories'] && @.operationType == 'CREATE_PROCESS')]"
          }
        }
      ]
    },
    {
      "name": "srcProcessFilePathHistogramLsassProcessDaily",
      "contextFieldNames": [
        "dstProcessFileName"
      ],
      "adeEventTypes": [
        "process"
      ],
      "strategyName": "fixed_duration_daily",
      "aggrFeatureConfs": [
        {
          "name": "srcProcessFilePathHistogram",
          "featureNamesMap": {
            "groupBy": [
              "srcProcessFilePath"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_histogram_func"
          },
          "filter": {
            "jsonPath": "[?('SYSTEM_32' in @['dstProcessDirectoryGroups'] && @.dstProcessFileName == 'lsass.exe' && @.operationType == 'CREATE_REMOTE_THREAD')]"
          }
        }
      ]
    }
  ]
}
