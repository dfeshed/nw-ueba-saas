{
  "BucketConfs": [
    {
      "name": "permissionChangeResultHistogramUserIdFileHourly",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "file"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "permissionChangeHistogram",
          "featureNamesMap": {
            "groupBy": [
              "result"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_histogram_func"
          },
          "filter": {
            "jsonPath": "[?(@.operationType == 'LOCAL_SHARE_PERMISSIONS_CHANGED' || @.operationType == 'FOLDER_ACCESS_RIGHTS_CHANGED' || @.operationType == 'FILE_ACCESS_RIGHTS_CHANGED' || @.operationType == 'FILE_CLASSIFICATION_CHANGED' || @.operationType == 'FOLDER_CLASSIFICATION_CHANGED' || @.operationType == 'FILE_CENTRAL_ACCESS_POLICY_CHANGED')]"
          }
        }
      ]
    },
    {
      "name": "fileActionResultHistogramUserIdFileHourly",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "file"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "fileActionHistogram",
          "featureNamesMap": {
            "groupBy": [
              "result"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_histogram_func"
          },
          "filter": {
            "jsonPath": "[?(@.operationType == 'FOLDER_OPENED' || @.operationType == 'FILE_DELETED' || @.operationType == 'FILE_OPENED' || @.operationType == 'FILE_RENAMED' || @.operationType == 'FILE_MOVED')]"
          }
        }
      ]
    },
    {
      "name": "fileOpenedAbsoluteSrcFilePathHistogramUserIdFileHourly",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "file"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "distinctOpenedFilesHistogram",
          "featureNamesMap": {
            "groupBy": [
              "absoluteSrcFilePath"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_histogram_func"
          },
          "filter": {
            "jsonPath": "[?(@.operationType == 'FILE_OPENED')]"
          }
        }
      ]
    },
    {
      "name": "folderOpenedAbsoluteSrcFolderFilePathHistogramUserIdFileHourly",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "file"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "distinctOpenedFoldersHistogram",
          "featureNamesMap": {
            "groupBy": [
              "absoluteSrcFolderFilePath"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_histogram_func"
          },
          "filter": {
            "jsonPath": "[?(@.operationType == 'FOLDER_OPENED')]"
          }
        }
      ]
    },
    {
      "name": "fileDeletedCounterUserIdFileHourly",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "file"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "deletedFilesCounter",
          "featureNamesMap": {},
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_sum_func"
          },
          "filter": {
            "jsonPath": "[?(@.operationType == 'FILE_DELETED')]"
          }
        }
      ]
    },
    {
      "name": "fileRenamedResultRenamedFilesHistogramUserIdFileHourly",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "file"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "successfulRenamedFilesHistogram",
          "featureNamesMap": {
            "groupBy": [
              "result"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_histogram_func"
          },
          "filter": {
            "jsonPath": "[?(@.operationType == 'FILE_RENAMED')]"
          }
        }
      ]
    },
    {
      "name": "fileMovedCounterUserIdFileHourly",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "file"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "movedFilesFromSharedDriveCounter",
          "featureNamesMap": {},
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_sum_func"
          },
          "filter": {
            "jsonPath": "[?(@.operationType == 'FILE_MOVED')]"
          }
        }
      ]
    },
    {
      "name": "fileMovedFromSharedDriveCounterUserIdFileHourly",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "file"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "movedFilesFromSharedDriveCounter",
          "featureNamesMap": {},
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_sum_func"
          },
          "filter": {
            "jsonPath": "[?(@.operationType == 'FILE_MOVED' && @.isSrcDriveShared == 'true' && @.isDstDriveShared == 'false')]"
          }
        }
      ]
    },
    {
      "name": "fileMovedToSharedDriveCounterUserIdFileHourly",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "file"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "movedFilesToSharedDriveCounter",
          "featureNamesMap": {},
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_sum_func"
          },
          "filter": {
            "jsonPath": "[?(@.operationType == 'FILE_MOVED' && @.isSrcDriveShared == 'false' && @.isDstDriveShared == 'true')]"
          }
        }
      ]
    }
  ]
}
