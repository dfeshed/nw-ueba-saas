{
  "BucketConfs": [
    {
      "name": "filePermissionChangeResultHistogramUserIdFileHourly",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "file"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "filePermissionChangeHistogram",
          "featureNamesMap": {
            "groupBy": [
              "result"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_histogram_func"
          },
          "filter": {
            "jsonPath": "[?(@.operationType == 'LOCAL_SHARE_PERMISSIONS_CHANGED' || @.operationType == 'FOLDER_ACCESS_RIGHTS_CHANGED' || @.operationType == 'FILE_ACCESS_RIGHTS_CHANGED' || @.operationType == 'FILE_CLASSIFICATION_CHANGED' || @.operationType == 'FOLDER_CLASSIFICATION_CHANGED' || @.operationType == 'FILE_CENTRAL_ACCESS_POLICY_CHANGED')]"
          }
        }
      ]
    },
    {
      "name": "fileActionResultHistogramUserIdFileHourly",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "file"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "fileActionHistogram",
          "featureNamesMap": {
            "groupBy": [
              "result"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_histogram_func"
          },
          "filter": {
            "jsonPath": "[?(@.operationType == 'FOLDER_OPENED' || @.operationType == 'FILE_DELETED' || @.operationType == 'FILE_OPENED' || @.operationType == 'FILE_RENAMED' || @.operationType == 'FILE_MOVED')]"
          }
        }
      ]
    },
    {
      "name": "absoluteSrcFilePathHistogramUserIdFileOpenedFileHourly",
