{
  "BucketConfs": [
    {
      "name": "reconnaissanceToolHistogramUserIdProcessHourly",
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "process"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "distinctReconnaissanceToolHistogram",
          "featureNamesMap": {
            "groupBy": [
              "dstProcessFileName",
              "dstProcessCategories"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_multi_key_histogram_func",
            "groupByValues": {
              "dstProcessCategories": [
                "RECONNAISSANCE_TOOL"
              ]
            }
          },
          "filter": {
            "jsonPath": "[?(@.operationType == 'CREATE_PROCESS')]"
          }
        }
      ]
    },
    {
      "name": "reconnaissanceToolDstProcessCategoriesHistogramUserIdProcessHourly",
      "contextFieldNames": [
        "userId"
      ],
      "adeEventTypes": [
        "process"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "dstProcessCategoriesHistogram",
          "featureNamesMap": {
            "groupBy": [
              "dstProcessCategories"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_multi_key_histogram_func",
            "groupByValues": {
              "dstProcessCategories": [
                "RECONNAISSANCE_TOOL"
              ]
            }
          },
          "filter": {
            "jsonPath": "[?(@.operationType == 'CREATE_PROCESS')]"
          }
        }
      ]
    },
    {
      "name": "reconnaissanceToolCounterUserIdDstProcessFileNameProcessHourly",
      "contextFieldNames": [
        "userId",
        "dstProcessFileName"
      ],
      "adeEventTypes": [
        "process"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "reconnaissanceToolCounter",
          "featureNamesMap": {},
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_sum_func"
          },
          "filter": {
            "jsonPath": "[?('RECONNAISSANCE_TOOL' in @['dstProcessCategories'] && @.operationType == 'CREATE_PROCESS')]"
          }
        }
      ]
    }
  ]
