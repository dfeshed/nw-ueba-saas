{
  "data-source-scorers": [
    {
      "data-source": "vpn-session",
      "scorers": [
        {
          "name": "vpn-session-events-scorer",
          "type": "pareto-scorer",

          "highest-score-weight": 0.65,
          "scorers": [
            {
              "name": "country-scorer",
              "type": "reducting-scorer",
              "reducting-weight": 0.5,
              "reducting-zero-score-weight": 0.95,

              "main-scorer": {
                "name": "country-main-scorer",
                "type": "model-scorer",

                "model": {
                  "name": "vpnsessionuser"
                },
                "number-of-samples-to-influence-enough": 30,
                "use-certainty-to-calculate-score": true
              },
              "reducting-scorer": {
                "name": "country-reducting-scorer",
                "type": "category-rarity-model-scorer",

                "model": {
                  "name": "vpnsessioncity"
                },
                "number-of-samples-to-influence-enough": 10,
                "minumum-number-of-discreet-values-to-influence": 5
              }
            },
            {
              "name": "hostname-scorer",
              "type": "model-scorer",

              "model": {
                "name": "vpnsessionuser"
              }
            },
            {
              "name": "data-buckets-model-scorer",
              "type": "low-values-score-reducer",

              "reduction-configuration": {"reductionConfigs":[{"reducingFeatureName":"readbytes","reducingFactor":0.8,"maxValueForFullyReduce":100000000,"minValueForNotReduce":500000000}]},
              "base-scorer": {
                "name": "data-bucket-model-scorer",
                "type": "continuous-model-scorer",

                "model": {
                  "name": "vpnsessionuser"
                }
              }
            }
          ]
        },
        {
          "name": "vpn-bytes-avg",
          "type": "model-scorer",
          "model": {
            "name": "vpnsessionuser"
          }
        }
      ]
    }
  ]
}