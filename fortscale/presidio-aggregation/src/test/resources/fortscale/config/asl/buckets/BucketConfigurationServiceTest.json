{
  "BucketConfs": [
    {
      "name": "normalized_username_dlpfile_hourly",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "context.normalizedUsername"
      ],
      "adeEventTypes": [
        "scored_enriched.dlpfile.date_time"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "highest_date_time_score",
          "featureNamesMap": {
            "maximize": [
              "score"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_feature_to_max_map_func"
          },
          "filter": {
            "jsonPath": "[?(@.score > 50)]"
          }
        }
      ]
    },
    {
      "name": "normalized_src_machine_dlpfile_daily",
      "expireAfterSeconds": 31536000,
      "contextFieldNames": [
        "context.normalizedUsername"
      ],
      "adeEventTypes": [
        "scored_enriched.dlpfile.normalizedSrcMachine"
      ],
      "strategyName": "fixed_duration_hourly",
      "aggrFeatureConfs": [
        {
          "name": "normalized_src_machine_to_highest_score_map",
          "featureNamesMap": {
            "groupBy": [
              "context.normalizedSrcMachine"
            ],
            "maximize": [
              "score"
            ]
          },
          "aggrFeatureFuncJson": {
            "type": "aggr_feature_feature_to_max_map_func"
          }
        }
      ]
    }
  ]
}
