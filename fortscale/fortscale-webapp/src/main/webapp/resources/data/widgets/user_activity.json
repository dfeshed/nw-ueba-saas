{
    "reportId": "user_vpn_logins_count_int",
    "title": "User Activity Summary",
    "refreshOn": ["vpnUser"],
    "views": [
        {
            "type": "table",
            "className": "widget-table-list",
            "settings": {
                "fields": [
                    {
                        "name": "Label",
                        "value": "{{label}}",
                        "map": {
                            "SUCCESS": "<strong>{{value}}</strong> successful <strong>VPN logins</strong> in the last 7 days",
                            "FAIL": "<strong>{{value}}</strong> failed <strong>VPN logins</strong> in the last 7 days"
                        }
                    }
                ],
                "noDataMessage": "No activity found"
            }
        }
    ]
}