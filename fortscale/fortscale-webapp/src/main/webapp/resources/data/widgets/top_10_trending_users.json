{
    "reportId": "top_10_trending_users",
    "title": "Top 10 Trending Users",
    "controls": [
        {
            "type": "link",
            "text": "View all",
            "href": "#/d/investigator",
            "refreshOn": ["currentScore"]
        }
    ],
    "views": [
        {
            "type": "table",
            "settings": {
                "fields": [
                    {
                        "name": "Username",
                        "value": "{{userName}}",
                        "link": "#/d/user/{{userId}}?currentFeature={{@currentScore}}"
                    },
                    {
                        "name": "Trend",
                        "value": "{{trend}}%",
                        "icon": {
                            "preset": "plusMinus",
                            "presetParams": {
                                "value": "trend"
                            }
                        }
                    },
                    {
                        "name": "Score",
                        "value": "{{score}}",
                        "style": "score",
                        "styleParams": {
                            "value": "score"
                        }
                    }
                ]
            }
        }
    ]
}