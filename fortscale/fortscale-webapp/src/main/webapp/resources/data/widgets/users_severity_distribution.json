{
    "className": "no-border no-margins",
    "style": { "margin-bottom": "none" },
    "reportId": "users_severity_distribution",
    "refreshOn": "currentScore",
    "views": [
        {
            "type": "percentChart",
            "settings": {
                "options": {
                    "height": "145px",
                    "width": "100%",
                    "labelsFont": {
                        "size": "20px"
                    },
                    "selectable": false,
                    "showSelectionBar": false
                },
                "series": [{
                    "field": "percentage",
                    "label": "{{percentage}}%",
                    "style": "severity",
                    "styleParams": {
                        "value": "name"
                    }
                }],
                "labels": {
                    "field": "percentage",
                    "value": "{{percentage}}",
                    "styleField": "percentage"
                }
            }
        }
    ]
}