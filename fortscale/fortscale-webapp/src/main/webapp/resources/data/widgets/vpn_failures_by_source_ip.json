{
    "reportId": "vpn_failures_by_source_ip",
    "title": " Top 10 Failed VPN Logins By Source IP",
    "controls": [
        {
            "type": "link",
            "text": "View all",
            "href": "#/d/investigator?entities=vpndata&status=FAIL",
            "refreshOn": ["loginsUser"]
        }
    ],
    "views": [
        {
            "type": "table",
            "settings": {
                "fields": [
                    {
                        "name": "Source IP",
                        "value": "{{source_ip}}",
                        "link": "#/d/investigator?entities=vpndata&source_ip={{source_ip}}&status=FAIL"
                    },
                    {
                        "name": "Failure Count",
                        "value": "{{failcount}}"
                    }
                ]
            }
        }
    ]
}