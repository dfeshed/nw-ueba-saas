{
    "reportId": "user_login_rate",
    "title": "Logins Rate",
    "refreshOn": ["loginsUser"],
    "views": [
        {
            "type": "barsChart",
            "settings": {
                "options": {
                    "height": "200px",
                    "width": "100%",
                    "createAxes": false,
                    "barLabelsFont": {
                        "size": 16,
                        "color": "#999999"
                    },
                    "labelsFont": {
                        "size": "14px",
                        "color": "#999999"
                    },
                    "labels": {
                        "height": 20,
                        "margin": 13,
                        "handleEvents": true
                    },
                    "selectable": false,
                    "padding": { "top": 0, "right": 50, "left": 50, "bottom": 0 },
                    "tooltip": "{{seriesLabel}}'s score for {{barLabel}}: {{barValue}}",
                    "values": {
                        "min": 0
                    }
                },
                "series": [
                    {
                        "field": "EventsCount",
                        "label": "{{@userName}}"
                    }
                ],
                "labels": {
                    "field": "Date",
                    "value": "{{Date}}",
                    "styleField": "Date"
                }
            }
        }
    ]
}