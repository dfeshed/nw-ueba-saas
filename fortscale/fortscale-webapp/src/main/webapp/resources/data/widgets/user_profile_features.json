{
    "title": "Anomalous User Features",
    "reportId": "user_profile_features",
    "controls": [
        {
            "type": "link",
            "text": "View all",
            "href": "#/d/user/{{@entityId}}/logins"
        }
    ],
    "views": [
        {
            "type": "table",
            "settings": {
                "rows": {
                    "style": "highScore",
                    "styleParams": {
                        "value": "featureScore"
                    }
                },
                "fields": [
                    {
                        "name": "Description",
                        "value": "{{featureDisplayName}}"
                    },
                    {
                        "name": "Value",
                        "value": "{{featureValue}}",
                        "sortBy": "value"
                    },
                    {
                        "name": "Feature Score",
                        "value": "{{featureScore}}",
                        "sortBy": "featureScore",
                        "sortDirection": -1
                    },
                    {
                        "name": "Average Score",
                        "value": "{{averageScore}}",
                        "sortBy": "averageScore",
                        "sortDirection": -1
                    }
                ]
            }
        }
    ]
}