{
    "reportId": "all_users_group_memberships",
    "title": "Suspicious Group Memberships",
    "linkTo": {
        "url": "#/d/investigator?entity=user_groups",
        "tooltip": "Investigate suspicious group memberships",
        "text": "View all"
    },
    "views": [
        {
            "type": "table",
            "settings": {
                "fields": [
                    {
                        "name": "User",
                        "value": "{{username}}",
                        "link": "#/d/user/{{username}}"
                    },
                    {
                        "name": "Group",
                        "value": "{{group}}",
                        "link": "#/d/investigator?entity=user_groups&group={{group}}"
                    },
                    {
                        "name": "Time",
                        "value": "{{time}}",
                        "format": "date",
                        "formatOptions": { "prettyDate": true }
                    },
                    {
                        "name": "Score",
                        "value": "{{score}}",
                        "style": "score",
                        "styleParams": {
                            "value": "score"
                        }
                    }
                ]
            }
        }
    ]
}