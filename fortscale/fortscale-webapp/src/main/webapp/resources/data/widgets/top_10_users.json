{
    "reportId": "top_10_users",
    "title": "Top 10 Suspicious Users",
    "controls": [
        {
            "type": "link",
            "text": "View all",
            "href": "#/d/investigator",
            "refreshOn": ["currentScore"]
        }
    ],
    "views": [
        {
            "type": "table",
            "settings": {
                "fields": [
                    {
                        "name": "Severity",
                        "icon": "stop",
                        "style": "score",
                        "styleParams": {
                            "value": "score"
                        },
                        "renderHeader": false,
                        "className": "table-small-cell"
                    },
                    {
                        "name": "User",
                        "value": "{{userName}}",
                        "link": "#/d/user/{{userId}}?currentFeature={{@currentScore}}",
                        "headerColspan": 2
                    },
                    {
                        "name": "Trend",
                        "value": "{{trend}}%",
                        "icon": {
                            "preset": "plusMinus",
                            "presetParams": {
                                "value": "trend"
                            }
                        }
                    },
                    {
                        "name": "Score",
                        "value": "{{score}}"
                    }
                ]
            }
        }
    ]
}