{
    "message":{
        "type": "maxResults",
        "params": {
            "maxResults": 50
        }
    },
    "views": [
        {
            "type": "bubbles",
            "settings": {
                "label": "{{label}}",
                "childIdField": "username",
                "value": "user_count",
                "tooltipText": "{{name}}: {{value}} {{value:pluralOrSingular:Machine:Machines}}",
                "onSelect": {
                    "action": "openPopup",
                    "actionOptions": {
                        "widgetId": "visualizations.user_machines_bubbles_popup",
                        "title": "Top 10 Machines Accessed by user: {{name}}",
                        "width": 500,
                        "height": 360,
						"params":{
							"name":"{{name}}"
						}
                    }
                }
            }
        }
    ]
}
