{
    "message":{
        "type": "maxResults",
        "params": {
            "maxResults": 50
        }
    },
    "views": [
        {
            "type": "bubbles",
            "settings": {
                "label": "{{severity}}",
                "childIdField": "username",
                "value": "user_count",
                "tooltipText": "{{name}} score: {{value}} {{value:pluralOrSingular:User:Users}}",
				"onSelect": {
					"action": "openPopup",
					"actionOptions": {
						"widgetId": "visualizations.score_users_bubbles_popup",
						"title": "Top 10 Users Who got {{name}} score",
						"width": 500,
						"height": 360,
						"params":{
							"name":"{{name}}"
						}
					}
				}
            }
        }
    ]
}
