{
    "title": "User Details",
    "className": "round-items-list",
    "views": [
        {
            "type": "properties",
            "settings": {
                "properties": [
                    {
                        "icon": "user",
                        "tooltip": "User name",
                        "value": "{{username}}"
                    },
                    {
                        "icon": "group",
                        "tooltip": "Member of",
                        "value": "{{name}}",
                        "array": "groups"
                    },
                    {
                        "icon": "sitemap",
                        "tooltip": "Manager",
                        "value": "{{manager.name}}",
                        "link": "#/d/user/{{manager.id}}"
                    },
                    {
                        "icon": "check",
                        "tooltip": "Active Directory Flags",
                        "booleanList": [
                            {
                                "label": "Account is Disabled",
                                "show": "accountIsDisabled"
                            },
                            {
                                "label": "Inter-domain trust account",
                                "show": "interdomainTrustAccountValue"
                            },
                            {
                                "label": "Locked out",
                                "show": "lockout"
                            },
                            {
                                "label": "No password required",
                                "show": "noPasswordRequiresValue"
                            },
                            {
                                "label": "Normal user account",
                                "show": "normalUserAccountValue"
                            },
                            {
                                "label": "Password expired",
                                "show": "passwordExpired"
                            },
                            {
                                "label": "Password never expires",
                                "show": "passwordNeverExpiresValue"
                            },
                            {
                                "label": "Server-trusted account",
                                "show": "serverTrustAccount"
                            },
                            {
                                "label": "Smart-card required",
                                "show": "smartcardRequired"
                            },
                            {
                                "label": "Trusted for delegation",
                                "show": "trustedForDelegation"
                            },
                            {
                                "label": "Trusted to authenticate for delegation",
                                "show": "trustedToAuthForDelegation"
                            },
                            {
                                "label": "Workstation-trusted account",
                                "show": "workstationTrustAccount"
                            }
                        ]
                    },
                    {
                        "icon": "clock",
                        "tooltip": "Account Expires",
                        "value": "{{adAcountExpires}}"
                    },
                    {
                        "icon": "info",
                        "tooltip": "Description",
                        "value": "{{description}}"
                    },
                    {
                        "icon": "envelope",
                        "tooltip": "Email",
                        "value": "{{email}}",
                        "link": "mailto:{{email}}"
                    },
                    {
                        "icon": "barcode",
                        "tooltip": "Employee ID",
                        "value": "{{adEmployeeID || adEmployeeNumber}}"
                    },
                    {
                        "icon": "globe",
                        "tooltip": "Department",
                        "value": "{{department}}"
                    },
                    {
                        "icon": "phone",
                        "tooltip": "Home phone",
                        "value": "{{homePhone}}"
                    },
                    {
                        "icon": "mobile-phone",
                        "tooltip": "Mobile phone",
                        "value": "{{mobilePhone}}"
                    },
                    {
                        "icon": "location-arrow",
                        "tooltip": "Address",
                        "value": "{{address}}"
                    }
                ]
            }
        }
    ]
}