{
    "title": "User Details",
    "className": "round-items-list",
    "views": [
        {
            "type": "properties",
            "settings": {
                "properties": [
                    {
                        "icon": "user",
                        "tooltip": "User name",
                        "value": "{{username}}"
                    },
                    {
                        "icon": "barcode",
                        "tooltip": "Worker ID",
                        "value": "{{workerId}}"
                    },
                    {
                        "icon": "group",
                        "tooltip": "Groups",
                        "value": "{{name}}",
                        "array": "groups"
                    },
                    {
                        "icon": "globe",
                        "tooltip": "Department",
                        "value": "{{department}}"
                    },
                    {
                        "icon": "phone",
                        "tooltip": "Home phone",
                        "value": "{{homePhone}}"
                    },
                    {
                        "icon": "mobile-phone",
                        "tooltip": "Mobile phone",
                        "value": "{{mobilePhone}}"
                    },
                    {
                        "icon": "location-arrow",
                        "tooltip": "Address",
                        "value": "{{address}}"
                    },
                    {
                        "icon": "sitemap",
                        "tooltip": "Manager",
                        "value": "{{manager.name}}",
                        "link": "#/d/user/{{manager.id}}"
                    }
                ]
            }
        }
    ]
}