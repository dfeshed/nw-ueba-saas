{
    "reportId": "vpn_failures_by_username",
    "title": "Top 10 Failed VPN Logins By Username",
    "controls": [
        {
            "type": "link",
            "text": "View all",
            "href": "#/d/investigator?entities=vpndata&status=FAIL",
            "refreshOn": ["loginsUser"]
        }
    ],
    "views": [
        {
            "type": "table",
            "settings": {
                "fields": [
                    {
                        "name": "Image",
                        "image": "data:image/gif;base64,{{userDetails.image}}",
                        "className": "table-avatar",
                        "renderHeader": false
                    },
                    {
                        "name": "User",
                        "value": "{{userDetails.name || username}}",
                        "link": "#/d/user/{{userDetails.id}}?currentFeature={{@currentScore}}",
                        "headerColspan": 2
                    },
                    {
                        "name": "Failure Count",
                        "value": "{{failcount}}"
                    }
                ]
            }
        }
    ]
}