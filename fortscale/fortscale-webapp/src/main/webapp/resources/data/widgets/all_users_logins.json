{
    "reportId": "all_users_logins",
    "title": "Suspicious Logins",
    "linkTo": {
        "url": "#/d/investigator?entities=wmievents4769",
        "tooltip": "Investigate user logins",
        "text": "View all"
    },
    "views": [
        {
            "type": "table",
            "settings": {
                "fields": [
                    {
                        "name": "User",
                        "value": "{{username}}",
                        "link": "#/d/user/{{userId}}"
                    },
                    {
                        "name": "Source IP",
                        "value": "{{sourceIp}}",
                        "link": "#/d/investigator?entities=wmievents4769&sourceip={{sourceIp}}"
                    },
                    {
                        "name": "Destination Hostname",
                        "value": "{{destinationHostname}}",
                        "link": "#/d/investigator?entities=wmievents4769&targetid={{destinationHostname}}"
                    },
                    {
                        "name": "Time",
                        "value": "{{eventTime}}",
                        "transform": {
                            "method": "date",
                            "options": {
                                "prettyDate": true
                            }
                        }
                    },
                    {
                        "name": "Score",
                        "value": "{{eventScore}}",
                        "style": "score",
                        "styleParams": {
                            "value": "eventScore"
                        }
                    }
                ]
            }
        }
    ]
}