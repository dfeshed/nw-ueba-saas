{
    "reportId": "all_users_logins",
    "title": "Suspicious Logins",
    "linkTo": {
        "url": "#/d/investigator?entity=logins",
        "tooltip": "Investigate user logins",
        "text": "View all"
    },
    "views": [
        {
            "type": "table",
            "settings": {
                "fields": [
                    {
                        "name": "User",
                        "value": "{{username}}",
                        "link": "#/d/user/{{userId}}",
                        "sortBy": "username"
                    },
                    {
                        "name": "Source IP",
                        "value": "{{sourceIp}}",
                        "sortBy": "sourceIp",
                        "link": "#/d/investigator?entity=logins&sourceIp={{sourceIp}}"
                    },
                    {
                        "name": "Destination Hostname",
                        "value": "{{destinationHostname}}",
                        "sortBy": "destinationHostname",
                        "link": "#/d/investigator?entity=logins&destinationIp={{destinationHostname}}"
                    },
                    {
                        "name": "Time",
                        "value": "{{eventTime}}",
                        "sortBy": "eventTime",
                        "sortDirection": -1,
                        "transform": {
                            "method": "date",
                            "options": {
                                "prettyDate": true
                            }
                        }
                    },
                    {
                        "name": "Score",
                        "value": "{{eventScore}}",
                        "style": "score",
                        "styleParams": {
                            "value": "eventScore"
                        }
                    }
                ]
            }
        }
    ]
}