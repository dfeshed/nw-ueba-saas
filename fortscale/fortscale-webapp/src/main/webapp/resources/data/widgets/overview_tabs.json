{
    "widgetId": "overview_tabs",
    "renderControls": false,
    "renderHeader": false,
    "className": "no-margins widget-advance-one",
    "views": [
        {
            "type": "tabs",
            "settings": {
                "tabs": [
                    {
                        "display": "Group Membership",
                        "id": "active_directory_group_membership"
                    },
                    {
                        "display": "Logins",
                        "id": "auth"
                    },
                    {
                        "display": "VPN",
                        "id": "vpn"
                    }
                ],
                "selectedTabId": "@currentScore",
                "events": [
                    {
                        "eventName": "select",
                        "action": "setParams",
                        "actionOptions": {
                            "updateUrl": true,
                            "params": {
                                "currentScore": "{{id}}"
                            }
                        }
                    }
                ]
            }
        }
    ]
}