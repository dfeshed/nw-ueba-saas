{
    "title": "Related Machines",
    "reportId": "user_machines",
    "views": [
        {
            "type": "pieChart",
            "settings": {
                "height": "150px",
                "width": "150px",
                "showInfo": true,
                "calculatePercent": true,
                "info": {
                    "title": "{{hostname}}",
                    "properties": [
                        {
                            "label": "Last Sign-in:",
                            "value": "{{lastSignIn}}",
                            "format": "date",
                            "formatOptions": {
                                "prettyDate": true
                            }
                        },
                        {
                            "label": "IP:",
                            "value": "{{ipAddress}}"
                        }
                    ]
                },
                "item": {
                    "value": "{{count}}%",
                    "label": "{{hostname}}"
                }
            }
        }
    ]
}