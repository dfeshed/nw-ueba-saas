{
	"endpoint": {
		"api":"dataQuery",
		"dataQuery":{
			"fields": [
				{
					"id": "destination_machine"
				},
				{
					"alias": "destination_machines_count",
					"func": {
						"name": "count",
						"params": {
							"all": true
						}
					}
				}
			],
			"entitiesJoin": {
				"joinType": "right",
				"entity": "users",
				"joinFields": {
					"left": "normalized_username",
					"right": "users.normalized_username"
				}
			},
			"conditions": {
				"operator": "AND",
				"terms": [
					{
						"type": "field",
						"id": "normalized_username",
						"operator": "equals",
						"valueParam": "normalized_username"
					}
				]
			},
			"groupBy": ["destination_machine"],
			"sort": {
				"field": { "alias": "destination_machines_count" },
				"direction": "DESC"
			}
		}
	},
	"params": [
		{
			"dashboardParam": "name",
			"field": "normalized_username",
			"value": "{{name}}"
		}

	]
}


