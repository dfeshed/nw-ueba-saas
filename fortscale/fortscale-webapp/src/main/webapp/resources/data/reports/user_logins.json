{
    "endpoint": {
        "entity": "classifier",
        "method": "events",
        "id": "login",
        "uid": "{{@username}}",
        "minScore": 50
    },
    "cache": "5m",
    "mock_data": "user.user_logins",
    "requiredParams": ["latestDate"],
    "params": [
        {
            "field": "latestDate",
            "dashboardParam": "date",
            "value":"{{date:date:valueOf:0d:end}}"
        },
        {
            "field": "earliestDate",
            "dashboardParam": "date",
            "value":"{{date:date:valueOf:-14d:start}}"
        },
        {
            "field": "username",
            "dashboardParam": "entityId"
        }
    ]
}