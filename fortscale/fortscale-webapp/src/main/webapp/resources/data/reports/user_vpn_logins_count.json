{
    "requiredParams": ["username"],
    "endpoint": {
        "entity": "classifier",
        "id": "vpn",
        "method": "eventsLoginCount",
        "username": "{{@username}}"
    },
    "params": [
        {
            "dashboardParam": "normalizedUsername",
            "field": "username"
        }
    ],
    "mock_data": "user.vpn_count",
    "process": {
        "processId": "combine",
        "params": {
            "groupBy": "day",
            "defaultValue": { "SUCCESS": 0, "FAILURE": 0 },
            "fields": {
                "status": "logincount"
            }
        }
    }
}