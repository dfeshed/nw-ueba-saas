{
	"_mock_data" : "user.user_machines_pairs",
	"endpoint": {
		"api": "dataQuery",
		"dataQuery": {
			"entity": "logins",
			"fields": [
				{
					"id": "source_machine",
					"alias": "hostname"
				},
				{
					"alias": "logoncount",
					"func": {
						"name": "count",
						"params": {
							"all": true
						}
					}
				},
				{
					"id": "event_time_utc",
					"alias": "lastlogon",
					"func": {
						"name": "max"
					}
				}
			],
			"groupBy": [
				"source_machine"
			],
			"conditions": {
				"operator": "AND",
				"terms": [
					{
						"type": "field",
						"id": "event_time_utc",
						"operator": "dateRange",
						"valueParam": "dateRange"
					},
					{
						"type": "field",
						"id": "normalized_username",
						"operator": "equals",
						"valueParam": "username"
					}
				]
			},
			"limit": 50,
			"sort": [
				{
					"field": "source_machine",
					"direction": "asc"
				}
			]
		}
	},
	"requiredParams": ["username"],
	"params": [
		{
			"field": "dateRange",
			"dashboardParam": "event_dates",
			"isRequired": true
		},
		{
			"dashboardParam": "normalizedUsername",
			"field": "username",
			"isRequired": true
		}
	]
}
