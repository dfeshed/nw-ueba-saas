{
    "searchId": "user_machines",
    "dataSource": "api",
    "query": {
        "entity": "users_machines",
        "conditions": [
            {
                "field": "username",
                "operator": "equals",
                "value": "{{username}}"
            }
        ]
    },
    "endpoint": {
        "entity": "user",
        "method": "machines"
    },
    "fields": [
        {
            "name": "machineName",
            "type": "string"
        },
        {
            "name": "ipAddress",
            "type": "string"
        },
        {
            "name": "usage",
            "type": "number"
        },
        {
            "name": "lastSignIn",
            "type": "date"
        },
        {
            "name": "machineScore",
            "type": "number"
        }
    ],
    "params": [
        {
            "field": "id",
            "name": "id",
            "dashboardParam": "entityId"
        }
    ]
}