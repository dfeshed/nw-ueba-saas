{
    "dashboardId": "user_active_directory_group_membership",
    "columns": [
        {
            "className": "span15",
            "widgets": [
                {
                    "widgetId": "user_score_history",
                    "title": "Group Membership Score History - Last 7 Days",
                    "report": {
                        "query": {
                            "params": [
                                {
                                    "dashboardParam": "entityId",
                                    "field": "username"
                                },
                                {
                                    "dashboardParam": "currentFeature",
                                    "field": "attribute"
                                }
                            ]
                        }
                    }
                },
                {
                    "widgetId": "user_profile_features",
                    "title": "Suspicious Group Membership",
                    "controls": [
                        {
                            "type": "link",
                            "text": "View all",
                            "href": "#/d/investigator?entities=ldap_group_membership_scores&username={{@loginsUser}}&time_stamp={{@date}}",
                            "refreshOn": ["date"]
                        }
                    ],
                    "refreshOn": "date",
                    "params": {
                        "date": null
                    }
                }
            ]
        }
    ]
}