{
    "dashboardId": "overview",
    "name": "Overview",
    "className": "dashboard-noBackground",
    "columns": [
        {
            "className": "span12",
            "widgets": [
                {
                    "title": "Suspicious Users",
                    "renderControls": false,
                    "renderHeader": false,
                    "className": "no-border widget-with-tabs",
                    "views": [
                        {
                            "type": "container",
                            "widgets": [
                                {
                                    "widgetId": "overview_tabs"
                                },
                                {
                                    "id": "overview_scoreDashboard",
                                    "renderControls": false,
                                    "renderHeader": false,
                                    "refreshOn": "currentScore",
                                    "className": "widget-no-top-padding",
                                    "show": {
                                        "conditions": [{
                                            "field": "{{currentScore}}",
                                            "operator": "hasValue"
                                        }]
                                    },
                                    "views": [
                                        {
                                            "type": "dashboardWidget",
                                            "className": "dashboard-with-border inner-widgets",
                                            "settings": {
                                                "dashboardId": "overview_score_{{@currentScore}}",
                                                "requiredParams": ["currentScore"]
                                            }
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "className": "span3",
            "widgets": [
            	{
            		"widgetId": "security_feed"            	
            	}
            ]		
    	}
    ]
}
