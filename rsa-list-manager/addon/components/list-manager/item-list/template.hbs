{{#if list}}
  {{#each list as |item|}}
    {{yield (hash
        item=(component 'list-manager/item-list/item'
          item=item
          selectedItem=selectedItem
          itemSelection=(action itemSelection item)
          isExpanded=isExpanded
          hasOOTBIndicators=hasOOTBIndicators
          highlightedId=highlightedId
          editItem=editItem
        )
       )
    }}
  {{/each}}
{{else}}
  <li class='no-results'>{{noResultsMessage}}</li>
{{/if}}

