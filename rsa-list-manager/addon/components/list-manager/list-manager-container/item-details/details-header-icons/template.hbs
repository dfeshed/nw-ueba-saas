{{!-- delete icon --}}
{{#if showDeleteIcon}}
  {{#rsa-form-button
    class='list-delete-icon'
    isDisabled=(not isDeletable)
    defaultAction=(action 'delete')
    title=titleMessage
  }}
    {{rsa-icon name='bin-1'}}
  {{/rsa-form-button}}
{{/if}}

{{!-- help icon --}}
{{#if hasContextualHelp}}
  {{#rsa-form-button title='Help' class='list-help-icon' defaultAction=(action 'goToHelp')}}
    {{rsa-icon name='help-circle'}}
  {{/rsa-form-button}}
{{/if}}
