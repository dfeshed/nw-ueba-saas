<hbox class="title-bar__file-title" fit>
  <div class="title-bar__file-name">{{summary.filename}}</div>
  <div class='divider'/>
</hbox>
<hbox>
  {{#each fileDetailTabs as |tab|}}
    {{#rsa-nav-tab isActive=tab.selected click=(action 'setNewFileTab' tab.name)}}
      <div class="label">{{t tab.label}}</div>
    {{/rsa-nav-tab}}
  {{/each}}
</hbox>
