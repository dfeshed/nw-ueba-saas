<hbox fit>
  {{spec-masthead model=model}}
</hbox>

<hbox>
  <fill block class="scroll-box">
  {{#spec-category title="Styles"}}
    {{#spec-member title="Filled"}}
      {{rsa-icon name='account-circle-1'}}
      {{!-- BEGIN-SNIPPET filled-icon
      {{rsa-icon name='account-circle-1'}}
      END-SNIPPET --}}
      {{code-snippet name="filled-icon.hbs"}}
    {{/spec-member}}

    {{#spec-member title="Lined"}}
      {{rsa-icon style='lined' name='account-circle-1'}}
      {{!-- BEGIN-SNIPPET lined-icon
      {{rsa-icon style='lined' name='account-circle-1'}}
      END-SNIPPET --}}
      {{code-snippet name="lined-icon.hbs"}}
    {{/spec-member}}
  {{/spec-category}}

  {{#spec-category title="Sizes"}}
    {{#spec-member title="Small"}}
      {{rsa-icon size='small' name='account-circle-1'}}
      {{!-- BEGIN-SNIPPET small-icon
      {{rsa-icon size='small' name='account-circle-1'}}
      END-SNIPPET --}}
      {{code-snippet name="small-icon.hbs"}}
    {{/spec-member}}

    {{#spec-member title="Large"}}
      {{rsa-icon size='large' name='account-circle-1'}}
      {{!-- BEGIN-SNIPPET large-icon
      {{rsa-icon size='large' name='account-circle-1'}}
      END-SNIPPET --}}
      {{code-snippet name="large-icon.hbs"}}
    {{/spec-member}}

    {{#spec-member title="Larger"}}
      {{rsa-icon size='larger' name='account-circle-1'}}
      {{!-- BEGIN-SNIPPET larger-icon
      {{rsa-icon size='larger' name='account-circle-1'}}
      END-SNIPPET --}}
      {{code-snippet name="larger-icon.hbs"}}
    {{/spec-member}}

    {{#spec-member title="Largest"}}
      {{rsa-icon size='largest' name='account-circle-1'}}
      {{!-- BEGIN-SNIPPET largest-icon
      {{rsa-icon size='largest' name='account-circle-1'}}
      END-SNIPPET --}}
      {{code-snippet name="largest-icon.hbs"}}
    {{/spec-member}}
  {{/spec-category}}
</fill>
</hbox>
