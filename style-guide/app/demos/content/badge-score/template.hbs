          </div>
        
          <code>
            &#123;{rsa-content-badge-score score=75 label='Risk Score' size='largest'}}
          </code>
        {{/spec-member}}
      {{/spec-category}}
    {{/rsa-grid-column}}
  {{/rsa-grid-row}}
{{/rsa-grid}}
