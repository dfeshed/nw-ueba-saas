{{#rsa-grid}}
  {{#rsa-grid-row collapseHeight=true}}
    {{#rsa-grid-column span=12}}
      {{spec-masthead model=model}}
    {{/rsa-grid-column}}
  {{/rsa-grid-row}}

  {{#rsa-grid-row}}
    {{#rsa-grid-column span=12 isScrollable=true}}
      {{#spec-category title="Examples"}}
        {{#spec-member title="With Single IP"}}
          {{rsa-content-ip-connections toIPs=model.singleIP fromIPs=model.singleIP}}

          <code>
            &#123;{rsa-content-ip-connections toIPs=model.singleIP fromIPs=model.singleIP}}
          </code>
        {{/spec-member}}

        {{#spec-member title="With Multiple IPs"}}
          {{rsa-content-ip-connections toIPs=model.multipleIPs fromIPs=model.multipleIPs}}

          <code>
            &#123;{rsa-content-ip-connections toIPs=model.multipleIPs fromIPs=model.multipleIPs}}
          </code>
        {{/spec-member}}
      {{/spec-category}}
    {{/rsa-grid-column}}
  {{/rsa-grid-row}}
{{/rsa-grid}}
