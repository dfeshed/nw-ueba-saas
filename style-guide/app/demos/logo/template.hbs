    {{#spec-member title="With Title and Version"}}
      {{rsa-logo title="Style Guide" version="1.0"}}
      {{!-- BEGIN-SNIPPET rsa-logo-with-version
      {{rsa-logo title="Style Guide" version="1.0"}}
      END-SNIPPET --}}
