{{#rsa-grid}}
  {{#rsa-grid-row collapseHeight=true}}
    {{#rsa-grid-column span=12}}
      {{spec-masthead model=model}}
    {{/rsa-grid-column}}
  {{/rsa-grid-row}}

  {{#rsa-grid-row}}
    {{#rsa-grid-column span=12 isScrollable=true}}
      {{#spec-category}}
        {{#spec-member title="Without Navigation"}}
          <code>
            &#123;{rsa-application-header}}
          </code>
        {{/spec-member}}
        
        {{#spec-member title="With Navigation"}}
          <code>
            <div>
              &#123;{#rsa-application-header}}
              <div>
                &#123;{#link-to "routeA"}}
                  Route A
                &#123;{/link-to}}
              </div>
              <div>
                &#123;{#link-to "routeB"}}
                  Route B
                &#123;{/link-to}}
              </div>
              &#123;{/rsa-application-header}}
            </div>
          </code>        
        {{/spec-member}}
      {{/spec-category}}
    {{/rsa-grid-column}}
  {{/rsa-grid-row}}
{{/rsa-grid}}



