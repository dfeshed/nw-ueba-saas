{{#each path as |step|}}

    {{#if (is-equal step.type "incidents-queue")}}

        {{rsa-incidents-queue
            cube=step.value onselect=(action "forward" step "incident-info") }}

    {{else if (is-equal step.type "incident-info")}}

        {{rsa-incident-info
            class="js-test-respond-inc-details"
            model=step.value }}

    {{/if}}

{{/each}}
{{yield this}}