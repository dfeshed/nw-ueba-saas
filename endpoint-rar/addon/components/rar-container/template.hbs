<iframe src="{{iframeSrc}}" class="download-rar-installer"></iframe>

{{#if enableRarPage}}
  <div class='endpoint-rar-header'>
    <h1>{{t 'endpointRAR.title'}}</h1>
  </div>

  {{#rsa-page-layout showRightZone=true as |layout|}}
    {{#layout.center}}

      {{rar-container/rar-installer serverId=serverId}}
      {{rar-container/rar-config}}

    {{/layout.center}}
    {{#layout.right}}

      {{endpoint/help-text-wrapper content=helpText}}

    {{/layout.right}}
  {{/rsa-page-layout}}
{{else }}
  {{rsa-panel-message message=(t 'endpointRAR.notPermitted')}}
{{/if}}
