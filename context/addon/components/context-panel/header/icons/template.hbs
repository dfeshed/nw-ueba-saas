<div>
  <span>
    {{#rsa-form-button
      isIconOnly=true title= (t 'context.header.help.title')
      defaultAction=goToHelp}}
      {{rsa-icon name="help-circle"}}
    {{/rsa-form-button}}
  </span>
  <span class="rsa-context-panel-header__close-icon">
    <button class="rsa-context-panel-header__panel-close" onclick={{action "closeAction"}} title={{t 'context.header.closeButton.title'}}>
      {{rsa-icon name='close'}}
    </button>
  </span>
</div>
