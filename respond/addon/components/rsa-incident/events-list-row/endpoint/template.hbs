<dl class="events-list-endpoint-header">
  <dt test-id="eventTimeLabel">{{t 'respond.eventsList.endpoint.time'}}</dt>
  <dd test-id="eventTimeValue">{{rsa-content-datetime timestamp=item.timestamp}}</dd>
  <dt test-id="eventTypeLabel">{{t 'respond.eventsList.endpoint.type'}}</dt>
  <dd test-id="eventTypeValue">{{item.type}}</dd>
  <dt test-id="eventCategoryLabel">{{t 'respond.eventsList.endpoint.category'}}</dt>
  <dd test-id="eventCategoryValue">{{item.category}}</dd>
  <dt test-id="eventActionLabel">{{t 'respond.eventsList.endpoint.action'}}</dt>
  <dd test-id="eventActionValue">{{item.action}}</dd>
  <dt test-id="eventHostnameLabel">{{t 'respond.eventsList.endpoint.hostname'}}</dt>
  <dd test-id="eventHostnameValue">{{item.hostname}}</dd>
  <dt test-id="eventUserAccountLabel">{{t 'respond.eventsList.endpoint.userAccount'}}</dt>
  <dd test-id="eventUserAccountValue">{{item.user_account}}</dd>
  <dt test-id="eventOperatingSystemLabel">{{t 'respond.eventsList.endpoint.operatingSystem'}}</dt>
  <dd test-id="eventOperatingSystemValue">{{item.operating_system}}</dd>
  <dt test-id="eventFileHashLabel">{{t 'respond.eventsList.endpoint.fileHash'}}</dt>
  <dd test-id="eventFileHashValue">{{item.file_SHA256}}</dd>
</dl>
<div class="events-list-endpoint-footer">
  <table>
    <caption class="sr-only" test-id="eventTableCaption">{{t 'respond.eventDetails.caption'}}</caption>
    <thead>
      <tr>
        <th></th>
        <th scope="col" test-id="eventTableFileNameLabel">{{t 'respond.eventsList.endpoint.tableFILENAME'}}</th>
        <th scope="col" test-id="eventTableLaunchLabel">{{t 'respond.eventsList.endpoint.tableLAUNCH'}}</th>
        <th scope="col" test-id="eventTablePathLabel">{{t 'respond.eventsList.endpoint.tablePATH'}}</th>
        <th scope="col" test-id="eventTableHashLabel">{{t 'respond.eventsList.endpoint.tableHASH'}}</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <th scope="row" test-id="eventSourceLabel">{{t 'respond.eventsList.endpoint.sourceLabel'}}</th>
        <td test-id="eventSourceFileNameValue">{{item.source_filename}}</td>
        <td test-id="eventSourceLaunchValue">{{item.source_parameter}}</td>
        <td test-id="eventSourcePathValue">{{item.source_path}}</td>
        <td test-id="eventSourceHashValue">{{item.source_hash}}</td>
      </tr>
      <tr>
        <th scope="row" test-id="eventTargetLabel">{{t 'respond.eventsList.endpoint.targetLabel'}}</th>
        <td test-id="eventTargetFileNameValue">{{item.target_filename}}</td>
        <td test-id="eventTargetLaunchValue">{{item.target_parameter}}</td>
        <td test-id="eventTargetPathValue">{{item.target_path}}</td>
        <td test-id="eventTargetHashValue">{{item.target_hash}}</td>
      </tr>
    </tbody>
  </table>
</div>
