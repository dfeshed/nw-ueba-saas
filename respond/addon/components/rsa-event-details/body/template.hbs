  model=model
  skipUnknowns=false
  order="timestamp, type, description, source, destination, domain, detector, size, data, related_links"
  hidden="enrichment, from, to, detected_by, file, indicatorId, id"
  scalarValueComponentClass="rsa-event-details/body-value"
  propertyNameFormatter=propertyNameFormatter
  as |property|
}}
  {{property.scalar}}
  {{#property.nested as |nested|}}
