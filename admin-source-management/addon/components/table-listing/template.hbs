{{#rsa-data-table
        items=items
        columnsConfig=columnsConfig}}
  {{#rsa-data-table/header as |column|}}
    {{column.title}}
  {{/rsa-data-table/header}}
  {{#rsa-data-table/body as |item index column|}}
    {{#rsa-data-table/body-cell item=item column=column}}
      {{#if (eq column.dataType 'DATE')}}
        {{#if (get item column.field)}}
          {{rsa-content-datetime outputFormat='MM/DD/YYYY' timestamp=(get item column.field)}}
        {{/if}}
      {{else}}
        {{get item column.field}}
      {{/if}}
    {{/rsa-data-table/body-cell}}
  {{/rsa-data-table/body}}
{{/rsa-data-table}}
<box class="items-footer">
  Items footer/toolbar
</box>