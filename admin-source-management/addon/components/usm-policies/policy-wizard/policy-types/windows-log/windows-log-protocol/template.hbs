<h4 class='title'>
  {{#rsa-text-with-tooltip value=tooltip
  style="standard"
  alwaysShow=true}}
    {{t 'adminUsm.policyWizard.windowsLogPolicy.protocol'}}
  {{/rsa-text-with-tooltip}}
  <span class="remove-setting"{{action 'removeFromSelectedSettings' 'protocol'}}>
    {{rsa-icon name="remove-circle-1" style="lined"}}
  </span>
</h4>

<div class="windows-log-protocol__list">
  {{#power-select
    options=protocolsList
    selected=selectedProtocol
    searchField=option
    onchange=(action 'updatePolicyProperty' 'protocol') as |option|
  }}
    {{option}}
  {{/power-select}}
</div>