<h4 class='title'>
  {{#rsa-text-with-tooltip value=tooltip
  style="standard"
  alwaysShow=true}}
    {{t 'adminUsm.policyWizard.edrPolicy.cpuMax'}}
  {{/rsa-text-with-tooltip}}
  <span class="remove-setting {{if isDefaultPolicy 'is-greyed-out' 'not-greyed-out'}}"
  {{action 'removeFromSelectedSettings' 'cpuMax'}}>
    {{rsa-icon name="remove-circle-1" style="lined"}}
  </span>
</h4>
{{range-slider
  start=[cpuMax]
  format=format
  min=5
  step=1
  connect=false
  on-change=(action 'updatePolicyProperty' 'cpuMax')
}}
