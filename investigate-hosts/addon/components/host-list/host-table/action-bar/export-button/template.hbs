{{#if isExportButtonDisabled.disabled}}
  {{#rsa-form-button isIconOnly=true isDisabled=true}}
    {{rsa-icon name="file-zipped" title=(t exportTitle) size="medium"}}
  {{/rsa-form-button}}
{{else}}
  {{#if (eq hostExportStatus "completed")}}
    {{#rsa-form-button isIconOnly=true click=(action "exportAsFile")}}
      {{rsa-icon name="file-zipped" title=(t "investigateHosts.hosts.button.exportCSV") size="medium"}}
    {{/rsa-form-button}}
  {{else}}
    {{#rsa-form-button isIconOnly=true isDisabled=true}}
      {{rsa-loader}}
    {{/rsa-form-button}}
  {{/if}}
{{/if}}
<iframe src={{exportLink}} class="export-machines-iframe"></iframe>
