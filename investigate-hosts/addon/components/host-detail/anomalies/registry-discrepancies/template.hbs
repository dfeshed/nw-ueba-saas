<hbox>
  {{#rsa-form-button
    class='close-filter'
    defaultAction=openFilterPanel
    isDisabled=true
  }}
    {{rsa-icon name='filter-2' size='small'}}
    <span>{{t 'investigateFiles.filter.filter'}}</span>
  {{/rsa-form-button}}
  {{#host-detail/utils/host-title-bar tabs=anomaliesTabs as |tab|}}
    {{#link-to (query-params subTabName=tab.name)}}{{t tab.label}}{{/link-to}}
  {{/host-detail/utils/host-title-bar}}
</hbox>
{{host-detail/utils/datatable
  items=(readonly registryDiscrepancies)
  type=(t 'investigateHosts.hosts.footerLabel.registryDiscrepancies')
  columnsConfig=columnsConfigWithTitle
  selectedIndex=-1
}}