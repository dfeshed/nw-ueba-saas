{{#if mftDirectory}}
  {{#each mftDirectory as |item|}}
    {{host-detail/downloads/directory-wrapper/mft-directory data=item value=item.name}}
  {{/each}}
{{else}}
  <span class='mft-directory-wrapper_loader'>{{rsa-loader size='small'}}</span>
{{/if}}


<div class='deleted-files'>
  <span>{{t 'investigateHosts.downloads.mftDirectory.deletedFiles'}}</span>
</div>

<div class='all-files'>
  <span>{{t 'investigateHosts.downloads.mftDirectory.allFiles'}}</span>
</div>