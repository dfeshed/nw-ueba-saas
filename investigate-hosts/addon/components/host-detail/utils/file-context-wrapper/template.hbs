<box xs='9'>
  {{endpoint/file-actionbar
    onSaveFileStatus = (action 'setFileContextFileStatus' tabName checksums)
    getSavedFileStatus = (action 'getFileContextFileStatus' tabName)
    serviceList=serviceList
    metaName='checksumSha256'
    itemList=fileContextSelections
    fileStatusData=statusData}}
  {{host-detail/utils/file-context-table isPaginated=isPaginated storeName=storeName tabName=tabName columnsConfig=columnsConfig}}
</box>
{{#if propertyConfig}}
  <box xs='3'>
    {{host-detail/utils/file-property-panel
      title='Property'
      localeNameSpace='investigateHosts.property.file'
      config=propertyConfig
      data=fileProperty}}
  </box>
{{/if}}
{{#if showServiceModal}}
  {{endpoint/pivot-to-investigate/modal
    serviceList=serviceList
    metaName='checksumSha256'
    item=item
    closeModal=(action 'onCloseServiceModal')}}
{{/if}}
