{{yield (hash
  localeNameSpace='investigateHosts.property.file'
  status=status
  tableItems=(readonly autoruns)
  type=(t 'investigateHosts.hosts.footerLabel.autoruns.autoruns')
  columnsConfig=columnsConfig
  selectRowAction=(action 'setSelectedRow')
  propertyConfig=propertyConfig
  propertyData=fileProperties
  propertyPanelTitle=(t 'investigateHosts.hosts.propertyPanelTitles.autoruns.autorun')
  isAllSelected=isAllSelected
  toggleAllItemSelectionAction=(action 'toggleAllAutorunSelection')
  toggleOneItemSelectionAction=(action 'toggleAutorunSelection')
  saveFileStatus = (action 'saveAutorunStatus' checksums)
  selectedFiles=selectedAutorunList
  selectedFileCount=selectedAutorunCount
  getSavedFileStatus=(action 'getSavedAutorunStatus' selectedAutorunList)
  fileStatusData=statusData
  )
}}
