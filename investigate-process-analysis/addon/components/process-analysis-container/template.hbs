{{#if hasError}}
  {{rsa-panel-message title=(t 'investigateShared.endpoint.errorTitle') message=errorMessage}}
{{else}}
  <div class="process-list-box processTreeBox {{if isEventsSelected 'collapse' 'expand'}}">
    {{process-tree queryInput=queryInput}}
  </div>

  <div class="process-list-box processDetailsBox {{unless isEventsSelected 'collapse' 'expand'}}">
   {{process-details}}
  </div>
{{/if}}
