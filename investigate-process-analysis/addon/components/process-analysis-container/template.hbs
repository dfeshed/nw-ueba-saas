{{#if hasError}}
  {{rsa-panel-message title=(t 'investigateShared.endpoint.errorTitle') message=errorMessage}}
{{else}}
  <vbox xs="2" class="process-list-box"></vbox>

  <vbox xs="7" class="process-list-box">
    {{process-tree queryInput=queryInput}}
    {{process-tree-legend}}
  </vbox>

  <vbox xs="3" class="process-list-box">
    {{process-property-panel
      title=(t 'investigateProcessAnalysis.property.title')
      data=fileProperties
      config=config
      localeNameSpace='investigateProcessAnalysis.property.file'}}
  </vbox>
{{/if}}
