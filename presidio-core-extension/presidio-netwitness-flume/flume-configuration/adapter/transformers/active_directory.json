      "type": "regex_captor_and_formatter",
      "name": "user-id-normalization",
      "sourceKey": "user_dst",
      "destinationKey": "userId",
      "captureAndFormatConfigurations": [
        {
          "pattern": "CN=([^,]+)",
          "format": "%s",
          "capturingGroupConfigurations": [
            {
              "index": 1,
              "caseFormat": "LOWER"
            }
          ]
        },
        {
          "pattern": "CN=([^,]+),.+",
          "format": "%s",
          "capturingGroupConfigurations": [
            {
              "index": 1,
              "caseFormat": "LOWER"
            }
          ]
        },
        {
          "pattern": "(.+\\\\)+(.+)@.+",
          "format": "%s",
          "capturingGroupConfigurations": [
            {
              "index": 2,
              "caseFormat": "LOWER"
            }
          ]
        },
        {
          "pattern": "(.+\\\\)+([^@]+)",
          "format": "%s",
          "capturingGroupConfigurations": [
            {
              "index": 2,
              "caseFormat": "LOWER"
            }
          ]
        },
        {
          "pattern": "(.+)@.+",
          "format": "%s",
          "capturingGroupConfigurations": [
            {
              "index": 1,
              "caseFormat": "LOWER"
            }
          ]
        },
        {
          "pattern": ".+",
          "format": "%s",
          "capturingGroupConfigurations": [
            {
              "index": 0,
              "caseFormat": "LOWER"
            }
          ]
        }
      ]
    },
