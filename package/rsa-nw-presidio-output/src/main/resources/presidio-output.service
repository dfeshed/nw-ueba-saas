[Unit]
Description=Presidio output REST API daemon
After= network.target

[Service]
User=presidio
Group=presidio
Type=simple
ExecStart=/usr/bin/java -jar /usr/sbin/presidio/output/presidio-output-webapp.jar
KillMode=mixed
KillSignal=SIGKILL
Restart=on-failure
RestartSec=5s
PrivateTmp=true

[Install]
WantedBy=multi-user.target
