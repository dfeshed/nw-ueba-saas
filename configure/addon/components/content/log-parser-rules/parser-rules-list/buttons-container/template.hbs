{{#if hasSelectedParserRule}}
  <hbox class="rsa-button-with-confirmation deleteRule">
    {{#rsa-button-with-confirmation
        title= (t 'configure.logsParser.modals.confirmDeleteRule')
        confirmButtonLabel=(t 'configure.logsParser.modals.ok')
        isDisabled= isOotb
        cancelButtonLabel=(t 'configure.logsParser.modals.cancel')
        onConfirm=( action 'deleteParserRule')
        confirmationMessage=(concat (t 'configure.logsParser.modals.okDeleteRule') ' \'' selectedParserRuleName '\'')}}
        {{t 'configure.logsParser.buttons.delete'}}
    {{/rsa-button-with-confirmation}}
  </hbox>
{{/if}}
